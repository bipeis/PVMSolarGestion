<section id='divGeneral' class='centradoListas'>
      <h1>Ficha Mantenimiento Tractora Carroceria</h1>
      <form id='formFichaMantenimiento' class='form' action=''>
            <article class='form-group'>
                  <label class='label label-default' for='id'>IdMantenimiento:</label>
                  <input id='id' type='number' readonly class='form-control' placeholder='id' required value= {{id}} >

                  <label class='label label-default' for='Tractora'>Tractora:</label>
                  <input id='Tractora' type='text' readonly class='form-control' placeholder='Tractora' required value= {{Tractora}} >

                  <label class='label label-default' for='Mantenimiento'>Mantenimiento:</label>
                  <input id='Mantenimiento' type='text' readonly class='form-control' placeholder='Mantenimiento' value= {{Mantenimiento }} >

                  <label class='label label-default' for='Taller'>Taller:</label>
                  <input id='Taller' type='text' readonly class='form-control' placeholder='Taller' value= {{Taller}} >

                  <label class='label label-default' for='Pieza'>Pieza:</label>
                  <input id='Pieza' type='text' readonly class='form-control' placeholder='Pieza' value= {{Pieza}} >

                  <label class='label label-default' for='Fecha'>Fecha:</label>
                  <input id='Fecha' type='text' readonly class='form-control' placeholder='Fecha' value= {{Fecha}} >

                  <label class='label label-default' for='Importe'>Importe:</label>
                  <input id='Importe' type='date' readonly class='form-control' placeholder='Importe' value= {{itv1}} >

                  <label class='label label-default' for='AccPropioTerceros'>AccPropioTerceros:</label>
                  <input id='AccPropioTerceros' type='date' readonly class='form-control' placeholder='AccPropioTerceros' value= {{AccPropioTerceros}} >

                  <label class='label label-default' for='Posicion'>AccAsegurado:</label>
                  <input id='PosiAccAseguradocion' type='date' readonly class='form-control' placeholder='AccAsegurado' value= {{AccAsegurado}} >

                  <label class='label label-default' for='AccImputableConductor'>AccImputableConductor:</label>
                  <input id='AccImputableConductor' type='date' readonly class='form-control' placeholder='AccImputableConductor' value= {{AccImputableConductor}} >

                  <label class='label label-default' for='Posicion'>Posicion:</label>
                  <input id='Posicion' type='date' readonly class='form-control' placeholder='Posicion' value= {{Posicion}} >

                  <label class='label label-default' for='Zona'>Zona:</label>
                  <input id='Zona' type='date' readonly class='form-control' placeholder='Zona' value= {{Zona}} >

                  <label class='label label-default' for='GastoTotal'>Gasto Total:</label>
                  <input id='GastosTotales' type='date' readonly class='form-control' placeholder='GastoTotal' value= {{GastoTotal}} >

            </article>

            <!--<input type='submit' value='Enviar' class='btn btn-default'>-->
            <!--<button id='modificarTractoraBtn' class='btn btn-default'>Modificar datos</button>-->
            <button id='editarMantenimientoBtn' data-id= {{id}} class='btn btn-default'>Editar</button>
            <button id='volverListaBtn' class='btn btn-default'>Volver a la lista</button>
      </form>
</section>